{
  "c++": {
    "optional_files": [
      ".gitignore",
      "README.md",
      "tests/test.cpp",
      "include/add.h"
    ],
    "commands": {
      "run": {
        "tasks": [
          {
            "name": "Build",
            "command": "make"
          },
          {
            "name": "Run",
            "command": "./build/main"
          }
        ],
        "args": null,
        "description": "Build and run the project"
      },
      "bin": {
        "tasks": [
          {
            "name": "Bin",
            "command": "./build/main"
          }
        ],
        "args": null,
        "description": "Run the project"
      },
      "test": {
        "tasks": [
          {
            "name": "Test",
            "command": "make tests"
          },
          {
            "name": "Run",
            "command": "./build/tests"
          }
        ],
        "args": null,
        "description": "Build and run the tests"
      }
    },
    "routes": []
  },
  "zig": {
    "optional_files": [
      ".gitignore",
      "README.md",
      "zig-cache/",
      "zig-out/"
    ],
    "commands": {
      "run": {
        "tasks": [
          {
            "name": "Build",
            "command": "zig build"
          },
          {
            "name": "Run",
            "command": "./zig-out/bin/main"
          }
        ],
        "args": null,
        "description": "Build and run the project"
      },
      "test": {
        "tasks": [
          {
            "name": "Test",
            "command": "zig build test"
          }
        ],
        "args": null,
        "description": "Build and run the tests"
      }
    },
    "routes": []
  },
  "golang": {
    "commands": {
      "run": {
        "tasks": [
          {
            "name": "Build",
            "command": "go build -o bin/main ./cmd/"
          },
          {
            "name": "Run",
            "command": "./bin/main"
          }
        ],
        "args": null,
        "description": "Build and run the project"
      },
      "test": {
        "tasks": [
          {
            "name": "Test",
            "command": "go test ./..."
          }
        ],
        "args": null,
        "description": "Build and run the tests"
      }
    },
    "optional_files": [
      ".gitignore",
      "README.md",
      "pkg",
      "test"
    ],
    "routes": []
  },
  "python": {
    "optional_files": [
      ".gitignore",
      "README.md"
    ],
    "commands": {
      "run": {
        "tasks": [
          {
            "name": "Run",
            "command": "python src/main.py"
          }
        ],
        "args": null,
        "description": "Run the project"
      },
      "test": {
        "tasks": [
          {
            "name": "Test",
            "command": "pytest"
          }
        ],
        "args": null,
        "description": "Run the tests"
      },
      "install": {
        "tasks": [
          {
            "name": "Install",
            "command": "pip install {{package}}"
          }
        ],
        "args": [
          {
            "name": "package",
            "description": "Package name"
          }
        ],
        "description": "Install a package"
      }
    },
    "routes": []
  },
  "typescript": {
    "optional_files": [
      ".gitignore",
      "README.md",
      "tsconfig.json",
      "bun.lockb"
    ],
    "commands": {
      "run": {
        "tasks": [
          {
            "name": "Run",
            "command": "bun run src/main.ts"
          }
        ],
        "args": null,
        "description": "Run the project"
      },
      "install": {
        "tasks": [
          {
            "name": "Install",
            "command": "bun install {{package}}"
          }
        ],
        "args": [
          {
            "name": "package",
            "description": "Package name"
          }
        ],
        "description": "Install a package"
      }
    },
    "routes": []
  }
